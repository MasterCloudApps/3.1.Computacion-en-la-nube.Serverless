{
    "name": "users-management-aws-cognito",
    "version": "1.0.0",
    "private": true,
    "description": "Users Management AWS Cognito Backend.",
    "main": "lambda.js",
    "license": "Apache-2.0",
    "dependencies": {
        "amazon-cognito-identity-js": "^3.2.1",
        "dotenv": "^8.2.0",
        "node-fetch": "^2.6.0"
    },
    "scripts": {
        "deploy-complete": "npm run create-bucket && npm run sam-package && npm run sam-deploy",
        "create-bucket": "aws s3api create-bucket --bucket users-management-bucket --create-bucket-configuration LocationConstraint=eu-west-2",
        "sam-package": "sam package --template-file template.yaml --s3-bucket users-management-bucket --output-template-file packaged-template.yaml",
        "sam-deploy": "sam deploy --template-file packaged-template.yaml --stack-name users-management-stack --capabilities CAPABILITY_IAM"
    }
}
